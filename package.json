{
  "name": "apirestpro",
  "version": "1.0.0",
  "description": "API Rest con nodejs y expressjs",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "dev": "NODE_ENV=development npx nodemon server.js",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "jest": {
    "bail": true,
    "verbose": true,
    "setupFiles": [
      "dotenv/config"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/*app.js",
      "!**/providers/**",
      "!**/coverage/**",
      "!server.js"
    ]
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "apirest"
  ],
  "author": "Juan Mora",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.5",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jest": "^25.3.0",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2"
  }
}
